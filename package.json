{
	"name": "sveltekit-minimalist-template",
	"version": "0.1.0",
	"type": "module",
	"engines": {
		"node": "18.16.0",
		"npm": "9.5.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/PaulioRandall/sveltekit-minimalist-template.git"
	},
	"scripts": {
		"fmt": "npx prettier --loglevel warn --write --plugin-search-dir=. .",
		"clean": "rm -rf .svelte-kit",
		"build": "npm run fmt && vite build",
		"dev": "npm run fmt && vite dev --port 3000",
		"preview": "npm run build && vite preview --port 3000",
		"commit": "npm run build && cat ./scripts/youre-ready-to-integrate.txt",
		"================================================================": "",
		"pipeline-build": "vite build"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/kit": "1.20.2",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"svelte": "3.59.1",
		"vite": "4.3.9"
	}
}
